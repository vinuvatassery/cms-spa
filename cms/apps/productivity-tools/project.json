{
  "projectType": "application",
  "root": "apps/productivity-tools",
  "sourceRoot": "apps/productivity-tools/src",
  "prefix": "cms",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/productivity-tools",
        "index": "apps/productivity-tools/src/index.html",
        "main": "apps/productivity-tools/src/main.ts",
        "polyfills": "apps/productivity-tools/src/polyfills.ts",
        "tsConfig": "apps/productivity-tools/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/productivity-tools/src/favicon.ico",
          "apps/productivity-tools/src/assets"
        ],
        "styles": [
          "libs/shared/util-theme/src/lib/styles.scss",
          "apps/productivity-tools/src/styles.scss"
        ],
        "scripts": [],
        "customWebpackConfig": {
          "path": "apps/productivity-tools/webpack.config.js"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/productivity-tools/src/environments/environment.ts",
              "with": "apps/productivity-tools/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "apps/productivity-tools/webpack.prod.config.js"
          }
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nrwl/angular:webpack-server",
      "configurations": {
        "production": {
          "browserTarget": "productivity-tools:build:production"
        },
        "development": {
          "browserTarget": "productivity-tools:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "port": 4204,
        "publicHost": "http://localhost:4204"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "productivity-tools:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/productivity-tools/src/**/*.ts",
          "apps/productivity-tools/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/productivity-tools"],
      "options": {
        "jestConfig": "apps/productivity-tools/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
