{
  "name": "system-config-feature-cases",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/system-config/feature-cases/src",
  "prefix": "system-config",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/system-config/feature-cases/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/system-config/feature-cases/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/system-config/feature-cases/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/system-config/feature-cases/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/system-config/feature-cases/**/*.ts",
          "libs/system-config/feature-cases/**/*.html"
        ]
      }
    }
  }
}
