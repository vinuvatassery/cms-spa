{
  "name": "productivity-tools-feature-notification",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/productivity-tools/feature-notification/src",
  "prefix": "productivity-tools",
  "tags": ["domain:productivity-tools", "type:feature"],
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": [
        "{workspaceRoot}/dist/libs/productivity-tools/feature-notification"
      ],
      "options": {
        "project": "libs/productivity-tools/feature-notification/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/productivity-tools/feature-notification/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/productivity-tools/feature-notification/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/libs/productivity-tools/feature-notification"
      ],
      "options": {
        "jestConfig": "libs/productivity-tools/feature-notification/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "libs/productivity-tools/feature-notification/src/**/*.ts",
          "libs/productivity-tools/feature-notification/src/**/*.html"
        ]
      }
    }
  }
}
