{
  "name": "productivity-tools-feature-approval",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/productivity-tools/feature-approval/src",
  "prefix": "productivity-tools",
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": [
        "{workspaceRoot}/dist/libs/productivity-tools/feature-approval"
      ],
      "options": {
        "project": "libs/productivity-tools/feature-approval/ng-package.json",
        "updateBuildableProjectDepsInPackageJson": true
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/productivity-tools/feature-approval/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/productivity-tools/feature-approval/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/libs/productivity-tools/feature-approval"
      ],
      "options": {
        "jestConfig": "libs/productivity-tools/feature-approval/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/productivity-tools/feature-approval/src/**/*.ts",
          "libs/productivity-tools/feature-approval/src/**/*.html"
        ]
      }
    }
  },
  "tags": ["domain:productivity-tools", "type:feature"]
}
