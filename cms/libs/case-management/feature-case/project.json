{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/case-management/feature-case/src",
  "prefix": "case-management",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["dist/libs/case-management/feature-case"],
      "options": {
        "project": "libs/case-management/feature-case/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/case-management/feature-case/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/case-management/feature-case/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/case-management/feature-case"],
      "options": {
        "jestConfig": "libs/case-management/feature-case/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/case-management/feature-case/src/**/*.ts",
          "libs/case-management/feature-case/src/**/*.html"
        ]
      }
    }
  },
  "tags": ["domain:case-management", "type:feature"]
}
