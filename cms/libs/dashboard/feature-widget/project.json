{
  "projectType": "library",
  "root": "libs/dashboard/feature-widget",
  "sourceRoot": "libs/dashboard/feature-widget/src",
  "prefix": "cms",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/dashboard/feature-widget"],
